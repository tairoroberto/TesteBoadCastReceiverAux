<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.testeboadcastreceiveraux"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.testeboadcastreceiveraux.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name="BroadcastReceiverFour" android:label="BroadcastReceiverFour">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"/>
                <action android:name="BROADCAST_RECEIVER_OUTRO_PROJETO"/>
            </intent-filter>
        </receiver>

   
    </application>

</manifest>
